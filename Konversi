package Projek1;

/**
 *
 * @author ROG
 */
public class Konversi {
    double fahrenheit(){
        return ((4 * Main.Awal)/5 + 32);
    }
    double reamur (){
        return ((4 * Main.Awal)/5);
    }
    double kelvin() {
        return (Main.Awal + 273.15);
    }
    
}
